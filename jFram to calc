    private void jButton4ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        int num100 =Integer.parseInt(num10.getText()) ;
        int num110 =Integer.parseInt(num11.getText()) ;
        int num120 =Integer.parseInt(num12.getText()) ;
        int num130 =Integer.parseInt(num13.getText()) ;
        int num140 =Integer.parseInt(num14.getText()) ;
        int h[] = {num100,num110,num120,num130,num140};
        int num200 =Integer.parseInt(num20.getText()) ;
        int num210 =Integer.parseInt(num21.getText()) ;
        int num220 =Integer.parseInt(num22.getText()) ;
        int num230 =Integer.parseInt(num23.getText()) ;
        int num240 =Integer.parseInt(num24.getText()) ;
        int g[] = {num200,num210,num220,num230,num240};
        StriOp me = new StriOp();
        int[] l = me.mulArry(g, h);
        jTextArea1.setText(String.valueOf("New Arry = { "+l[0]+" , "+l[1]+" , "+l[2]+" , "+l[3]+" , "+l[4]+" }"));
        
    }                                        

    private void jButton1ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        float num100 =Integer.parseInt(num10.getText()) ;
        float num110 =Integer.parseInt(num11.getText()) ;
        float num120 =Integer.parseInt(num12.getText()) ;
        float num130 =Integer.parseInt(num13.getText()) ;
        float num140 =Integer.parseInt(num14.getText()) ;
        float h[] = {num100,num110,num120,num130,num140};
        float num200 =Integer.parseInt(num20.getText()) ;
        float num210 =Integer.parseInt(num21.getText()) ;
        float num220 =Integer.parseInt(num22.getText()) ;
        float num230 =Integer.parseInt(num23.getText()) ;
        float num240 =Integer.parseInt(num24.getText()) ;
        float g[] = {num200,num210,num220,num230,num240};
        StriOp me = new StriOp();
        float[] l = me.divArry(g, h);
        jTextArea1.setText(String.valueOf("New Arry = { "+l[0]+" , "+l[1]+" , "+l[2]+" , "+l[3]+" , "+l[4]+" }"));
    }                                        

